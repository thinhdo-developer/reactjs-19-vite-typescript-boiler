{
  "name": "tiktak",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev_win": "set APP_ENV=development&& vite --force --mode development",
    "dev": "APP_ENV=development vite --force --mode development",
    "qa": "APP_ENV=qa vite --force --mode qa",
    "qa_win": "set APP_ENV=qa&& vite --force --mode qa",
    "prod": "APP_ENV=production vite --force --mode production",
    "prod_win": "set APP_ENV=production&& vite --force --mode production",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && APP_ENV=development vite build --mode development",
    "build:dev_win": "tsc -b && set APP_ENV=development&& vite build --mode development",
    "build:qa": "tsc -b && APP_ENV=qa vite build --mode qa",
    "build:qa_win": "tsc -b && set APP_ENV=qa&& vite build --mode qa",
    "build:prod": "tsc -b && APP_ENV=production vite build --mode production",
    "build:prod_win": "tsc -b && set APP_ENV=production&& vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.png"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@reduxjs/toolkit": "^2.3.0",
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.59.16",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "formik": "^2.4.9",
    "i18next": "25.6.3",
    "i18next-browser-languagedetector": "^8.0.0",
    "lucide-react": "^0.555.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "16.3.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "7.9.6",
    "react-select": "^5.10.2",
    "react-toastify": "^11.0.5",
    "register-service-worker": "^1.7.2",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vite-pwa/assets-generator": "1.0.2",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "16.5.0",
    "jsdom": "27.2.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.1.17",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.14",
    "vitest-sonar-reporter": "3.0.0"
  }
}